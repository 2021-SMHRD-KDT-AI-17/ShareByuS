<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.smhrd.mapper.G_BoardMapper">

	<select id="getGBoard" resultType="kr.smhrd.entity.g_board">
		select * from g_board
	</select>

	<insert id="insertBoard" parameterType="kr.smhrd.entity.g_board">
		INSERT INTO g_board (g_num,g_title,g_writer,email,g_img1,g_content,g_w_date,g_b_date,category)
		VALUES(1,#{g_title},'g_writer 1',#{email},#{g_img1},#{g_content},NOW(),NOW(),#{category})
	</insert>




</mapper>